<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="viewModel"
            type="com.tuofeng.bskyhunchbacktransport.viewmodel.activity.WayBillTaskViewModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:orientation="vertical">

        <include layout="@layout/titlelbar_layout" />

        <ViewStub
            android:id="@+id/view_stub_data"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout="@layout/activity_way_bill_task_data" />

        <ViewStub
            android:id="@+id/view_stub_no_data"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout="@layout/activity_way_bill_task_no_data" />
    </LinearLayout>
</layout>
